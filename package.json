{
  "name": "agentic-extension",
  "displayName": "Agentic VS Code Assistant",
  "description": "An AI agent that analyzes your project and implements user stories using LangChain + LLMs.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agentic.start",
        "title": "Start Agentic Assistant"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@types/ws": "^8.18.1",
    "typescript": "^5.2.0",
    "ws": "^8.18.3"
  }
}
